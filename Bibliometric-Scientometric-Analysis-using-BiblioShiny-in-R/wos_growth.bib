@article{ WOS:000780192800015,
Author = {Arshed, Noman and Hanif, Nadia and Aziz, Osama and Croteau, Martin},
Title = {Exploring the potential of institutional quality in determining
   technological innovation},
Journal = {TECHNOLOGY IN SOCIETY},
Year = {2022},
Volume = {68},
Month = {FEB},
Abstract = {Having better institutions facilitates many aspects of the economy. But
   this increase, assuming other factors, may lead to a gradual decrease in
   its effectiveness to promote innovation. The preposition behind this is
   that mature institutions increase businesses' compliance costs.
   Governments should try to find an appropriate intervention approach to
   promote innovation. This study posits to evaluate the non-linear effect
   of development in institutional quality on innovation. Panel FGLS model
   confirms the Innovation - Institutional quality Kuznets curve for four
   sets of countries segregated according to their level of development as
   very high HDI, high HDI, medium HDI, and low HDI. The study finds that
   all sets of countries witnessing an inverted U-relationship between
   development in institutional quality and innovation, pointing to the
   preposition of burdening effect of regulations. Further, this burden is
   eased with the increase in the economic development level as low HDI
   country's curve bends higher as compared to very high HDI countries.
