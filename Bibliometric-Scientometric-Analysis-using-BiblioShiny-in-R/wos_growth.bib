@article{ WOS:000807620900001,
Author = {Kamga, Benjamin Fomba and Fokam, Dieu Ne Dort Talla and Nchofoung, Tii
   N.},
Title = {Internet access and innovation in developing countries: some empirical
   evidence},
Journal = {TRANSNATIONAL CORPORATIONS REVIEW},
Year = {2022},
Month = {2022 JUN 3},
Abstract = {The main objective of this study is to analyse the effect of Internet
   access on innovation and to examine the economic mechanisms through
   which this effect is possible. The empirical analyses cover a sample of
   29 developing countries over the period from 2001 to 2018. The main
   results obtained using the pooled cross-sectional time-series model with
   generalised least squares random-effects estimator show that Internet
   access has a positive effect on innovation. Regarding the role of
   transmission channels, the results show that Internet access interacts
   with foreign direct investment and financial development producing
   positive synergy effects on innovation. Based on these results, it is
   obvious that strengthening innovation capacity in developing countries
   requires investments in Internet technologies to enable a larger
   population to access the Internet connection.},
