@article{ WOS:000560290700012,
Author = {Shankar, Venkatesh and Narang, Unnati},
Title = {Emerging market innovations: unique and differential drivers,
   practitioner implications, and research agenda},
Journal = {JOURNAL OF THE ACADEMY OF MARKETING SCIENCE},
Year = {2020},
